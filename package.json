    "nprogress": "^0.2.0",
    "pinia": "^2.2.4",
    "pinia-plugin-persistedstate": "^4.1.2",
    "ts-md5": "^1.3.1",
    "vue": "^3.5.10",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@gx-web/eslint-config": "^2.0.1",
    "@gx-web/prettier-config": "^1.0.1",
